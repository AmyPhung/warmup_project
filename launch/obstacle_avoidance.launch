<launch>
  <!-- Obstacle Avoidance Node -->
  <node type="obstacle_avoidance.py" name="obstacle_avoidance" pkg="warmup_project"
    output="screen"/>

  <!-- RVIZ Node -->
  <node type="rviz" name="rviz" pkg="rviz"
    args="-d $(find warmup_project)/rviz/obstacle_avoidance_config.rviz" />

  <!-- Dynamic Reconfigure -->
  <node name = "rqt_dashboard" pkg = "rqt_gui" type = "rqt_gui"
    respawn = "false" output = "screen"
    args = "--perspective-file $(find warmup_project)/rqt_config/obstacle_avoidance.perspective"/>

</launch>
